<indexterm zone="geoclue-address-details-copy"><primary>geoclue_address_details_copy</primary></indexterm><programlisting><link linkend="GHashTable">GHashTable</link>*         geoclue_address_details_copy        (<link linkend="GHashTable">GHashTable</link> *source);</programlisting>
<para>
